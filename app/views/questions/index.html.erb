<%# Страница редактирования вопроса %>
<div class="container content-wrapper buffer-both">
  <h1 class="title-medium">Вопросы и ответы<%= " c \##{@selected_hashtag_value}" unless @selected_hashtag_value.nil?%></h1>


    <div class="user-questions buffer-both">
      <div class="questions-list">
        <% @selected_questions.each do |question| %>
          <%= render partial: 'questions/question', object: question, locals: {select_tag: "\##{@selected_hashtag_value}"} %>
        <% end %>
      </div>
    </div>

</div>